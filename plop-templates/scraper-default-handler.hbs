import { PuppeteerCrawlingContext } from 'crawlee'

export const defaultHandler = async ({
    enqueueLinks,
    log,
    request,
}: PuppeteerCrawlingContext): Promise<void> => {
    log.info(request.url)

    await enqueueLinks({
        globs: ['https://www.{{lowercase name}}.com/{{lowercase name}}s/**'],
        label: '{{constantCase name}}_PAGE',
        limit: 5,
    })
}
